{
  "name": "@hpcc-js/wasm",
  "version": "0.2.0",
  "description": "hpcc-js - WASM Libraries",
  "main": "dist/index.js",
  "module": "dist/index.es6",
  "unpkg": "dist/index.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*",
    "types/*",
    "src/*.css"
  ],
  "scripts": {
    "install-prerequisites": "run-script-os",
    "install-prerequisites:linux": "echo nop",
    "install-prerequisites:win32": "wsl -u root -e ./scripts/cpp-install-prerequisites.sh",
    "install-emsdk": "run-script-os",
    "install-emsdk:linux": "./scripts/cpp-install-emsdk.sh",
    "install-emsdk:win32": "wsl -e ./scripts/cpp-install-emsdk.sh",
    "install-graphviz": "run-script-os",
    "install-graphviz:linux": "./scripts/cpp-install-graphviz.sh",
    "install-graphviz:win32": "wsl -e ./scripts/cpp-install-graphviz.sh",
    "install-build-deps": "run-s install-prerequisites install-emsdk install-graphviz",
    "update-emsdk": "wsl -e ./scripts/cpp-update-emsdk.sh",
    "clean": "rimraf coverage build dist lib* types *.tsbuildinfo",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "compile-cpp": "run-script-os",
    "compile-cpp:linux": "./scripts/cpp-build.sh",
    "compile-cpp:win32": "wsl -e ./scripts/cpp-build.sh",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "terser dist/index.js -c -m --source-map -o dist/index.min.js",
    "build": "run-s compile-cpp compile-es6 bundle minimize",
    "publish-version": "standard-version",
    "publish-push": "git push --follow-tags upstream",
    "prepublishOnly": "run-s clean build publish-version publish-push",
    "watch": "run-p compile-es6-watch bundle-watch",
    "dev-start": "ws",
    "lint": "tslint --project . src/**/*.ts",
    "karma": "karma start",
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "promise-worker": "2.0.1"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "coveralls": "3.0.4",
    "karma": "4.1.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-coveralls": "2.1.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "local-web-server": "3.0.1",
    "mocha": "6.1.4",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "rollup": "1.15.4",
    "rollup-plugin-alias": "1.5.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.2",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-sourcemaps": "0.4.2",
    "run-script-os": "1.0.3",
    "standard-version": "6.0.1",
    "terser": "4.0.0",
    "tslib": "1.10.0",
    "tslint": "5.17.0",
    "typedoc": "0.14.2",
    "typescript": "3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GordonSmith/hpcc-js-wasm.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GordonSmith/hpcc-js-wasm/issues"
  },
  "homepage": "https://github.com/GordonSmith/hpcc-js-wasm#readme"
}